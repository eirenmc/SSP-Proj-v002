extends layout

block content
    div.container
        h1 #{username}  Portfolio
        
        -var values = project;
        ol
            each val in values ? values : ['There are no values']
                li
                    strong
                        p Project Title: 
                    p #{val.title}
                    strong
                        p Project Description:
                    p #{val.desc}
                    p #{val.tags}
                br
